{
    "name": "rayiumir/laravel-slugable",
    "description": "Minimal auto slug trait for Laravel Eloquent models.",
    "keywords": [
        "laravel-slugable",
        "laravel slugable",
        "easy slug",
        "laravel",
        "laravel slugable package"
    ],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Raymond Baghumian",
            "email": "rayiumir@gmail.com",
            "homepage": "https://github.com/Rayiumir",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rayiumir\\Slugable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rayiumir\\Slugable\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rayiumir\\Slugable\\ServiceProvider\\SlugServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
